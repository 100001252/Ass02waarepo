        <bpel:variable name="getBookLinkRequest" messageType="ns0:getBookRequest"></bpel:variable>
    </bpel:variables>
    <bpel:sequence name="main">
		
        <bpel:receive name="receiveInputs" partnerLink="clientPartnerLink" operation="checkBookExistence" portType="tns:BookBPELPortType" variable="clientPartnerLinkRequest" createInstance="yes"></bpel:receive>
        
