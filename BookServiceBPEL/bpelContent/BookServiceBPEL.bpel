</bpel:literal></bpel:from>
                <bpel:to variable="getBookLinkRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="clientPartnerLinkRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[id]]>
                    </bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="getBookLinkRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns0:bookId]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="getBookService" partnerLink="getBookLink" operation="getBook" portType="ns0:Ass02BookSoapPortType" inputVariable="getBookLinkRequest" outputVariable="getBookLinkResponse"></bpel:invoke>
        <bpel:assign validate="no" name="Assign1">
            <bpel:copy>
                <bpel:from><bpel:literal><ns:checkBookExistenceResponse xmlns:ns="http://au.edu.swin.waa" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <result>result</result>
</ns:checkBookExistenceResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="clientPartnerLinkResponse" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="getBookLinkResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns0:return]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="clientPartnerLinkResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutputs" partnerLink="clientPartnerLink" operation="checkBookExistence" portType="tns:BookBPELPortType" variable="clientPartnerLinkResponse"></bpel:reply>
        
        
        
    </bpel:sequence>    
